<?xml version="1.0"?>
<robot name="my_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

   
    <xacro:property name="base_mass" value="0.1"/> 
    <xacro:property name="base_radius" value="0.20"/>
    <xacro:property name="base_length" value="0.16"/>

    <xacro:property name="wheel_mass" value="0.1"/>
    <xacro:property name="wheel_radius" value="0.025"/>
    <xacro:property name="wheel_length" value="0.02"/>
    <xacro:property name="wheel_joint_y" value="0.19"/>
    <xacro:property name="caster_radius" value="0.025"/>

    <link name="base_footprint"/>
   

    <joint name="base_joint" type="fixed">
        <parent link="base_footprint"/>
        <child link="base_link"/>
        <origin rpy="0 0 0 " xyz="7 4 0.025"/>
    </joint>
        
    
    <link name="base_link">
        
        <visual>
            <geometry>
                <box size="0.25 0.16 0.05"/>
            </geometry>
            <origin rpy="0 0 0 " xyz="0 0 0 "/>
            <!-- <material name="blue">
                <color rgba="0 0 0.8 1"/>
            </material> -->
        </visual>
        
        <collision>
            <origin rpy="0 0 0 " xyz="0 0 0 "/>
            <geometry>
                <box size="0.25 0.16 0.05"/>
            </geometry>
        </collision>

        <inertial>
            <origin rpy="0 0 0 " xyz="0 0 0 "/>
            <mass value="${base_mass}"/>
            <inertia 
                ixx="0.0001" 
                iyy="0.0001" 
                ixy="0"
                ixz="0"
                iyz="0"
                izz="0.0001"/>
        </inertial>
    </link>

    <joint name="left_wheel_joint1" type="continuous">
        <origin xyz="0.1 0.09 -0.03" rpy="1.5707 0 0"/>
        <parent link="base_link"/>
        <child link="left_wheel_link1"/>
        <axis xyz="0 0 -1"/>
    </joint>

    <link name="left_wheel_link1">
        <visual>
            <geometry>
                <cylinder radius="${wheel_radius}" length="${wheel_length}"/>
            </geometry>
            <material name="black">
                <color rgba="0 0 0 1"/>
            </material>
        </visual>
       
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0 "/>
            <geometry>
                <cylinder radius="${wheel_radius}" length="${wheel_length}"/>
            </geometry>
        </collision>
       
        <inertial>
           <mass value="${wheel_mass}"/>
            <inertia 
                ixx="0.0001" 
                iyy="0.0001" 
                ixy="0"
                ixz="0"
                iyz="0"
                izz="0.0001"/>
            <origin xyz="0 0 0" rpy="0 0 0 "/>
       
        </inertial>

    </link>

    <joint name="right_wheel_joint1" type="continuous">
        <origin xyz="0.1 -0.09 -0.03" rpy="1.5707 0 0"/>
        <parent link="base_link"/>
        <child link="right_wheel_link1"/>
        <axis xyz="0 0 -1"/>
    </joint>

    <link name="right_wheel_link1">
        <visual>
            <geometry>
                <cylinder radius="${wheel_radius}" length="${wheel_length}"/>
            </geometry>
            <material name="black">
                <color rgba="0 0 0 1"/>
            </material>
        </visual>
        
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0 "/>
            <geometry>
                <cylinder radius="${wheel_radius}" length="${wheel_length}"/>
            </geometry>
        </collision>
       
        <inertial>
            <mass value="${wheel_mass}"/>
            <inertia 
                ixx="0.0001" 
                iyy="0.0001" 
                ixy="0"
                ixz="0"
                iyz="0"
                izz="0.0001"/>
            <origin xyz="0 0 0" rpy="0 0 0 "/>
        </inertial>

    </link>

    <joint name="left_wheel_joint2" type="continuous">
    <origin xyz="-0.1 0.09 -0.03" rpy="1.5707 0 0"/>
    <parent link="base_link"/>
    <child link="left_wheel_link2"/>
    <axis xyz="0 0 -1"/>
</joint>

<link name="left_wheel_link2">
    <visual>
        <geometry>
            <cylinder radius="${wheel_radius}" length="${wheel_length}"/>
        </geometry>
        <material name="black">
            <color rgba="0 0 0 1"/>
        </material>
    </visual>

    <collision>
        <origin xyz="0 0 0" rpy="0 0 0 "/>
        <geometry>
            <cylinder radius="${wheel_radius}" length="${wheel_length}"/>
        </geometry>
    </collision>

    <inertial>
        <mass value="${wheel_mass}"/>
        <inertia 
            ixx="0.0001" 
            iyy="0.0001" 
            ixy="0"
            ixz="0"
            iyz="0"
            izz="0.0001"/>
        <origin xyz="0 0 0" rpy="0 0 0 "/>
    </inertial>
</link>

<joint name="right_wheel_joint2" type="continuous">
    <origin xyz="-0.1 -0.09 -0.03" rpy="1.5707 0 0"/>
    <parent link="base_link"/>
    <child link="right_wheel_link2"/>
    <axis xyz="0 0 -1"/>
</joint>

<link name="right_wheel_link2">
    <visual>
        <geometry>
            <cylinder radius="${wheel_radius}" length="${wheel_length}"/>
        </geometry>
        <material name="black">
            <color rgba="0 0 0 1"/>
        </material>
    </visual>

    <collision>
        <origin xyz="0 0 0" rpy="0 0 0 "/>
        <geometry>
            <cylinder radius="${wheel_radius}" length="${wheel_length}"/>
        </geometry>
    </collision>

    <inertial>
        <mass value="${wheel_mass}"/>
        <inertia 
            ixx="0.0001" 
            iyy="0.0001" 
            ixy="0"
            ixz="0"
            iyz="0"
            izz="0.0001"/>
        <origin xyz="0 0 0" rpy="0 0 0 "/>
    </inertial>
</link>

<link name="laser_link">
    <visual>
        <geometry>
            <cylinder length="0.06" radius="0.04"/>
        </geometry>
        <material name="white">
            <color rgba="1 1 1 1"/>
        </material>
    </visual>
</link>

<joint name="laser_joint" type="fixed">
    <parent link="base_link"/>
    <child link="laser_link"/>
    <origin xyz="0 0.0 0.06"/>
</joint>


